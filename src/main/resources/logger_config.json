{
  "uriBlacklist":[
    "/arbiter/health",
    "/arbiter/api/v1/disputes/batches/templates",
    "/arbiter/api/v1/disputes/batches/files/*",
    "/arbiter/api/v1/dnds/cases/*/documents/*"
  ],
  "maskables":[
    {
      "requestUri":"*",
      "maskItems":[
        {
          "path":"annotation.request.evidence.*.base64EncodedBinary",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.response.evidence.*.base64EncodedBinary",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.request.base64EncodedBatchFile",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.response.cardlessPan",
          "maskPoints":[6,-4]
        },
        {
          "path":"annotation.response.customerPan",
          "maskPoints":[6,-4]
        },
        {
          "path":"annotation.response.postilionPan",
          "maskPoints":[6,-4]
        },
        {
          "path":"annotation.*.transactionReference",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.*.pan",
          "maskPoints":[6,-4]
        },
        {
          "path":"annotation.*.originalTransactionReference",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.*.duplicateTransactionReferences",
          "maskPoints":[1,0]
        },
        {
          "path":"annotation.*.base64EncodedImage",
          "maskPoints":[1,0]
        }
      ]
    }
  ],
  "fieldsToBeExtracted":[
    "annotation.header",
    "annotation.requestUri",
    "annotation.responseStatus",
    "annotation.request",
    "annotation.response"
  ]
}